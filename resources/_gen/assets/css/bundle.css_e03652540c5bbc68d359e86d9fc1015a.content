/* Custom styles for the header */
.header .navbar-brand span:first-child{
  color: #ffa500;
}

.header .container{
    padding-left: 10px;
}

html body ::selection{
  background-color:#ffa500;
  color:#fff;
}

.btn-primary{
  background-color: --var(--primary);
}

.skill__title {
    font-size: 24px;
    font-weight: 600;
    position: relative;
    z-index: 2;
}

blockquote{
  background:#EDEDED;
  border-left:8px solid #ffa500 ;
  color: #ffa500;
  font-family: "Open Sans";
  font-size: 1.4em;
  font-style:italic;
  line-height:1.6;
  margin:10px auto;
  padding:1.2em 30px 1.2em 75px;
  position: relative;
  width:90%;
}

blockquote::before{
  color:#ffa500;
  content: "\201C";
  font-family:Arial;
  font-size:4em;
  left: 10px;
  position: absolute;
  top:-10px;
}

blockquote::after{
  content: '';
}

blockquote span{
  color:#333;
  display:block;
  font-style: normal;
  font-weight: bold;
  margin-top:1em;
}

/* Общие стили для блока кода */
pre {
  background-color: #272822;
  border-radius: 5px;
  color: #f8f8f2;
  font-family: 'Fira Code', 'Courier New', monospace;
  font-size: 14px;
  line-height: 1.5;
  overflow: auto;
  padding: 10px;
}

/* Стиль для inline-блока кода */
code {
  background-color: #272822;
  border-radius: 3px;
  color: #fff;
  font-family: inherit;
  padding: 0 5px;
}

/* Стили для подсветки синтаксиса */
code .comment { color: #75715e; } /* Комментарии */
code .string { color: #e6db74; }  /* Строки */
code .keyword { color: #f92672; } /* Ключевые слова */
code .function { color: #66d9ef; } /* Функции */
code .number { color: #ae81ff; } /* Числа */

/* Скролл-бар для длинного кода */
pre::-webkit-scrollbar {
  width: 8px;
}

pre::-webkit-scrollbar-thumb {
  background-color: #44475a;
  border-radius: 5px;
}

pre::-webkit-scrollbar-track {
  background-color: #272822;
}

.badge-container {
    display: flex;
    flex-wrap: wrap; /* Ensures responsiveness */
    gap: 20px; /* Space between badges */
    justify-content: center; /* Centers badges in the container */
}

.badge-card {
    background: white;
    border-radius: 10px;
    box-shadow: 0 4px 6px rgb(0 0 0 / 10%);
    font-family: Arial, sans-serif;
    padding: 20px;
    text-align: center;
    transition: transform 0.2s ease-in-out;
    width: 185px;
}

.badge-card img {
    margin-bottom: 10px;
    max-width: 60px;
}

.badge-title {
    color: #333;
    font-size: 16px;
    font-weight: bold;
}

.badge-org {
    color: #666;
    font-size: 14px;
    margin-bottom: 10px;
}

.badge-date {
    color: #999;
    font-size: 12px;
}

.badge-card:hover {
    transform: scale(1.50); /* Adds a hover effect */
}

@media (prefers-color-scheme: dark) {
  .badge-card {
    background: #333;
    color: #fff;
  }

  .badge-title {
    color: #fff;
  }

  .badge-org {
    color: #ccc;
  }

  .badge-date {
    color: #aaa;
  }
}