/* dark mode */
@media (prefers-color-scheme: dark) {
    /* General Styles */
    html body, body {
        background-color: #181818;
        color: #fff;
    }

    body {
        background-color: #121212;
        color: #f5f5f5;
    }

    header,
    footer,
    .section {
        background-color: #1e1e1e;
    }

    a {
        color: #ffa500;
    }

    /* Typography */
    .h1, .h2, .h3, .h4, .h5, .h6,
    h1, h2, h3, h4, h5, h6,
    .display-1,
    .lead,
    p a, span a,
    .education__title,
    .contact__info span,
    .btn-frameless {
        color: #fff;
    }

    p {
        color: #a6a6a6;
    }

    .posts-list article.summary {
        color: #c3c3c3;
    }

    /* Education Section */
    .education__degree,
    .contact__info h3 {
        color: #a6a6a6;
    }

    .education__date {
        color: #d5d5d5;
    }

    .education::before {
        background-color: #202020;
    }

    /* Experience Section */
    .experience__company {
        color: #ffa500;
    }

    .experience__date {
        color: #a6a6a6;
    }

    .rad-showcase .platform-links [class^="icon-"] {
        filter: invert(100%);
    }

    .experience a {
        color: inherit;
    }

    /* Testimonial Section */
    .testimonial__author-info span {
        color: #F18983;
    }

    /* Contact Section */
    .section--contact .container {
        background-color: #181818;
    }

    /* Buttons */
    .btn-primary {
        background-color: #fff;
        color: #000;
    }

    .btn-primary:hover {
        border-color: #fff;
        color: #fff;
    }

    .btn-frameless.disabled,
    .btn-frameless.focus,
    .btn-frameless:disabled,
    .btn-frameless:focus,
    .btn-frameless:hover,
    .btn-frameless:not(:disabled, .disabled).active,
    .btn-frameless:not(:disabled, .disabled):active,
    .show>.btn-frameless.dropdown-toggle {
        background-color: #fff;
        border-color: #fff;
        color: #181818;
    }

    /* Inputs */
    input[type="text"],
    input[type="email"],
    textarea {
        background-color: #181818;
        border-bottom: 1px solid #fff;
        color: #fff;
    }

    input[type="text"]::placeholder,
    input[type="email"]::placeholder,
    textarea::placeholder {
        color: #6e6e6e;
    }

    input[type="text"]:focus,
    input[type="text"]:active,
    input[type="email"]:focus,
    input[type="email"]:active,
    textarea:focus,
    textarea:active {
        border-bottom: 2px solid #fff;
    }

    /* Navbar */
    .header {
        background-color: #1e1e1e;
    }

    .navbar,
    .navbar-light {
        background-color: transparent;
    }

    .nav-link {
        color: #e0e0e0;
    }

    .nav-link.active {
        color: #fff;
    }

    .navbar-toggler-icon {
        filter: invert(100%);
    }

    /* Lists */
    .section li,
    li>ul>li,
    li>ol>li {
        color: #f5f5f5;
    }

    /* Images */
    .cert-img {
        filter: brightness(0.9) contrast(1.1);
    }

    .header .navbar-brand span:nth-child(2), .nav-item {
        filter: invert(100%);
    }

    /* article.summary h1 a {
        color: #ffa500;
    } */

    blockquote {
        background-color: transparent !important;
    }

    blockquote p {
        color: #fff;
    }
}

